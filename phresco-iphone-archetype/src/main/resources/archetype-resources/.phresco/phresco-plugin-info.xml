<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
        <goal>package</goal>
        <description>Goal which packages the Iphone project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>com.photon.phresco.plugins.xcode.XcodePlugin</implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Build Name</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildName</key>
					<multiple>false</multiple>
                    <value></value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>Number</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildNumber</key>
					<multiple>false</multiple>
                    <value></value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Environment</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.EnvironmentParameter</class>
                        <dependencies>
                            <dependency>
								<groupId>com.photon.phresco.commons</groupId>
								<artifactId>phresco-commons</artifactId>
								<type>jar</type>
								<version>1.0.0</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
					<multiple>true</multiple>
                    <value>Production</value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Show Settings</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>showSettings</key>
                    <multiple>false</multiple>
                    <value>true</value>
                    <dependency>environmentName</dependency>
                </parameter>
				  <parameter>
					<pluginParameter>plugin</pluginParameter>
					<mavenCommands>
						<mavenCommand>
							<key>iphonesimulator5.1</key>
							<value>-Dsdk="iphonesimulator5.1"</value>
						</mavenCommand>
						<mavenCommand>
							<key>iphoneos5.1</key>
							<value>-Dsdk="iphoneos5.1"</value>
						</mavenCommand>
					</mavenCommands>
                    <name>
                        <value lang="en">SDK</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.plugins.xcode.IosSdkParameter</class>
                        <dependencies>
                            <dependency>
								<groupId>com.photon.phresco.commons</groupId>
								<artifactId>phresco-commons</artifactId>
								<type>jar</type>
								<version>1.0.0</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>sdk</key>
					<multiple>false</multiple>
                    <value></value>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>Show Errors</key>
                            <value>-e</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>Hide Logs</key>
                            <value>-q</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>Show Debug</key>
                            <value>-X</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>logs</key>
                    <possibleValues>
                        <value>Show Errors</value>
                        <value>Hide Logs</value>
                        <value>Show Debug</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value></value>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
    <mojo>
        <goal>deploy</goal>
        <description>Goal which deploys the Drupal-6 project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>com.photon.phresco.plugins.drupal.DrupalPlugin</implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Environment</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.DeployEnvironment</class>
                        <dependencies>
                            <dependency>
<groupId>com.photon.phresco.commons</groupId>
<artifactId>phresco-commons</artifactId>
<type>jar</type>
<version>1.0.0</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
					<multiple>false</multiple>
                    <value>Production</value>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>Show Errors</key>
                            <value>-e</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>Hide Logs</key>
                            <value>-q</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>Show Debug</key>
                            <value>-X</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>logs</key>
                    <possibleValues>
                        <value>Show Errors</value>
                        <value>Hide Logs</value>
                        <value>Show Debug</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value></value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>Hidden</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildNumber</key>
					<multiple>false</multiple>
                    <value></value>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
	<mojo>
      <goal>validate-code</goal>
      <description>Goal which validates the Drupal-6 project</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
	  <configuration>
      <parameters>
        <parameter>
		<pluginParameter>plugin</pluginParameter>
			<mavenCommands>
				<mavenCommand>
					<key>skipTests</key>
					<value>-DskipTests</value>
				</mavenCommand>
			</mavenCommands>
			<name>codeValidation</name>
			<type>java.lang.String</type>
			<required>false</required>
			<editable>true</editable>
			<description></description>
			<key>sonar</key>
			<possibleValues>
				<value dependency="src,skipTests">src</value>
				<value>functional</value>
			</possibleValues>
			<multiple>false</multiple>
		  <value></value>
        </parameter>
		<parameter>
			<name>src</name>
			<mavenCommands>
				<mavenCommand>
					<key>js</key>
					<value>-Pjs</value>
				</mavenCommand>
				<mavenCommand>
					<key>java</key>
					<value>-Pjava</value>
				</mavenCommand>
				<mavenCommand>
					<key>html</key>
					<value>-Phtml</value>
				</mavenCommand>
				<mavenCommand>
					<key>jsp/jsf</key>
					<value>-Pweb</value>
				</mavenCommand>
			</mavenCommands>
			<pluginParameter>plugin</pluginParameter>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
		  <key>src</key>
			<possibleValues>
				<value>js</value>
				<value>java</value>
				<value>html</value>
				<value>jsp/jsf</value>
			</possibleValues>
			<multiple>false</multiple>
		  <value></value>
        </parameter>
          </parameters>
      </configuration>
    </mojo>
	<mojo>
        <goal>unit-test</goal>
        <description>Goal which Unit Test for Iphone project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>com.photon.phresco.plugins.xcode.XcodePlugin</implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
					<pluginParameter>plugin</pluginParameter>
					<mavenCommands>
						<mavenCommand>
							<key>iphonesimulator5.1</key>
							<value>-Dsdk="iphonesimulator5.1"</value>
						</mavenCommand>
						<mavenCommand>
							<key>iphoneos5.1</key>
							<value>-Dsdk="iphoneos5.1"</value>
						</mavenCommand>
					</mavenCommands>
                    <name>
                        <value lang="en">SDK</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.plugins.xcode.IosSdkParameter</class>
                        <dependencies>
                            <dependency>
								<groupId>com.photon.phresco.commons</groupId>
								<artifactId>phresco-commons</artifactId>
								<type>jar</type>
								<version>1.0.0</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>sdk</key>
					<possibleValues>
						<value key="iphonesimulator5.1">iphonesimulator5.1</value>
						<value key="iphoneos5.1">iphoneos5.1</value>
					</possibleValues>
					<multiple>false</multiple>
                    <value>iphonesimulator5.1</value>
                </parameter>
				<parameter>
					<pluginParameter>plugin</pluginParameter>
					<mavenCommands>
						<mavenCommand>
							<key>iphonesimulator5.1</key>
							<value>-Dsdk="iphonesimulator5.1"</value>
						</mavenCommand>
						<mavenCommand>
							<key>iphoneos5.1</key>
							<value>-Dsdk="iphoneos5.1"</value>
						</mavenCommand>
					</mavenCommands>
                    <name>
                        <value lang="en">Target</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>target</key>
					<possibleValues>
						<value key="HelloWorld">HelloWorld</value>
						<value key="alltest">AllTest</value>
					</possibleValues>
					<multiple>false</multiple>
                    <value>AllTest</value>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
</mojos>
