<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
        <goal>functional-test-UIAutomation</goal>
        <description>Goal which Functional Test for Iphone Natice Project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>
           <dependency>
               <groupId>com.photon.phresco.plugins</groupId>
               <artifactId>xcode-phresco-plugin</artifactId>
               <type>jar</type>
               <version>2.1.0.2000</version>
               <class>com.photon.phresco.plugins.xcode.XcodePlugin</class>
           </dependency>
       </implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.impl.IosShowFunctionalDeviceIdValidationImpl</class>
                        <dependencies>
                            <dependency>
                                <groupId>com.photon.phresco.framework</groupId>
                                <artifactId>phresco-framework-impl</artifactId>
                                <type>jar</type>
                                <version>2.1.0.2000</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildNumber</key>
                    <multiple>false</multiple>
                    <value>1</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Device Id</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>deviceId</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
</mojos>