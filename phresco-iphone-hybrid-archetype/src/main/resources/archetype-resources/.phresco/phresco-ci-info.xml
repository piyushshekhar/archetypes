<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
        <goal>package</goal>
        <description>Goal which packages the Iphone Hybrid Project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>
           <dependency>
               <groupId>com.photon.phresco.plugins</groupId>
               <artifactId>xcode-phresco-plugin</artifactId>
               <type>jar</type>
               <version>2.0.0.35003-SNAPSHOT</version>
               <class>com.photon.phresco.plugins.xcode.XcodePlugin</class>
           </dependency>
       </implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Build Name</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildName</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Project Type</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>projectType</key>
                    <multiple>false</multiple>
                    <value>xcode</value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>Number</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildNumber</key>
                    <multiple>false</multiple>
                    <value>1</value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Show Settings</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>showSettings</key>
                    <multiple>false</multiple>
                    <value>false</value>
                    <sort>false</sort>
                    <show>true</show>
                    <dependency>environmentName</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Environment</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.impl.EnvironmentsParameterImpl</class>
                        <dependencies>
                            <dependency>
                                <groupId>com.photon.phresco.commons</groupId>
                                <artifactId>phresco-commons</artifactId>
                                <type>jar</type>
                                <version>2.0.0.35003-SNAPSHOT</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">SDK</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.impl.IosSdkParameterImpl</class>
                        <dependencies>
                            <dependency>
                                <groupId>com.photon.phresco.framework</groupId>
                                <artifactId>phresco-framework-impl</artifactId>
                                <type>jar</type>
                                <version>2.0.0.35003-SNAPSHOT</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>sdk</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Target</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.impl.IosTargetParameterImpl</class>
                        <dependencies>
                            <dependency>
                                <groupId>com.photon.phresco.framework</groupId>
                                <artifactId>phresco-framework-impl</artifactId>
                                <type>jar</type>
                                <version>2.0.0.35003-SNAPSHOT</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>target</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Mode</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>mode</key>
                    <possibleValues>
                        <value key="Debug">Debug</value>
                        <value key="Release">Release</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Encrypt</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>encrypt</key>
                    <multiple>false</multiple>
                    <value>false</value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">PlistFile</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>plistFile</key>
                    <multiple>false</multiple>
                    <value>Phresco/phresco-env-config.xml</value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>showErrors</key>
                            <value>-e</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>hideLogs</key>
                            <value>-q</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>showDebug</key>
                            <value>-X</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>logs</key>
                    <possibleValues>
                        <value key="showErrors">Show Errors</value>
                        <value key="hideLogs">Hide Logs</value>
                        <value key="showDebug">Show Debug</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
    <mojo>
        <goal>deploy</goal>
        <description>Goal which deploys the Iphone Hybrid Project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>
           <dependency>
               <groupId>com.photon.phresco.plugins</groupId>
               <artifactId>xcode-phresco-plugin</artifactId>
               <type>jar</type>
               <version>2.0.0.35003-SNAPSHOT</version>
               <class>com.photon.phresco.plugins.xcode.XcodePlugin</class>
           </dependency>
       </implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>simulator</key>
                            <value>-Ddevice.deploy=false</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>device</key>
                            <value>-Ddevice.deploy=true</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Device Type</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>deviceType</key>
                    <possibleValues>
                        <value key="simulator" dependency="sdkVersion,family">Simulator</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value>simulator</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Simulator</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.impl.IosSimSDKVersionsParameterImpl</class>
                        <dependencies>
                            <dependency>
                                <groupId>com.photon.phresco.framework</groupId>
                                <artifactId>phresco-framework-impl</artifactId>
                                <type>jar</type>
                                <version>2.0.0.35003-SNAPSHOT</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>sdkVersion</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Family</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>family</key>
                    <possibleValues>
                        <value key="iphone">iphone</value>
                        <value key="ipad">ipad</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>showErrors</key>
                            <value>-e</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>hideLogs</key>
                            <value>-q</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>showDebug</key>
                            <value>-X</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>logs</key>
                    <possibleValues>
                        <value key="showErrors">Show Errors</value>
                        <value key="hideLogs">Hide Logs</value>
                        <value key="showDebug">Show Debug</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>Hidden</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildNumber</key>
                    <multiple>false</multiple>
                    <value>1</value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Trigger Simulator</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>triggerSimulator</key>
                    <multiple>false</multiple>
                    <value>false</value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
    <mojo>
        <goal>functional-test-UIAutomation</goal>
        <description>Goal which Functional Test-webdriver for Iphone Hybrid Project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>
           <dependency>
               <groupId>com.photon.phresco.plugins</groupId>
               <artifactId>xcode-phresco-plugin</artifactId>
               <type>jar</type>
               <version>2.0.0.35003-SNAPSHOT</version>
               <class>com.photon.phresco.plugins.xcode.XcodePlugin</class>
           </dependency>
       </implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>Hidden</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildNumber</key>
                    <multiple>false</multiple>
                    <value>1</value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
    <mojo>
        <goal>CIPrebuildStep</goal>
        <description>Goal which do's CI pre build step</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>
           <dependency>
               <groupId>com.photon.phresco.plugins</groupId>
               <artifactId>ci-phresco-plugin</artifactId>
               <type>jar</type>
               <version>2.0.0.35003-SNAPSHOT</version>
               <class>com.photon.phresco.plugins.CIPlugin</class>
           </dependency>
       </implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
    </mojo>
</mojos>