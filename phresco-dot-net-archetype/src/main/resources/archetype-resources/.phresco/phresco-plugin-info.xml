<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
        <goal>package</goal>
        <description>Goal which packages the .net project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>com.photon.phresco.plugins.dotnet.DotnetPlugin</implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Build Name</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildName</key>
					<multiple>false</multiple>
					<sort>false</sort>
					<show>true</show>
                    <value></value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>Number</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildNumber</key>
					<multiple>false</multiple>
					<sort>false</sort>
					<show>true</show>
                    <value></value>
                </parameter>
				<parameter>
                    <name>
                        <value lang="en">Show Settings</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>showSettings</key>
                    <multiple>false</multiple>
					<sort>false</sort>
					<show>true</show>
                    <value>true</value>
                    <dependency>environmentName</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Environment</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
					<class>com.photon.phresco.framework.param.EnvironmentParameter</class>                       
                        <dependencies>
                            <dependency>
								<groupId>com.photon.phresco.commons</groupId>
								<artifactId>phresco-commons</artifactId>
								<type>jar</type>
								<version>1.0.0</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
					<multiple>true</multiple>
					<sort>false</sort>
					<show>false</show>
                    <value></value>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>showErrors</key>
                            <value>-e</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>hideLogs</key>
                            <value>-q</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>showDebug</key>
                            <value>-X</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>logs</key>
                    <possibleValues>
                        <value key="showErrors">Show Errors</value>
                        <value key="hideLogs">Hide Logs</value>
                        <value key="showDebug">Show Debug</value>
                    </possibleValues>
                    <multiple>false</multiple>
					<sort>false</sort>
					<show>true</show>
                    <value></value>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
    <mojo>
        <goal>deploy</goal>
        <description>Goal which deploys the .net project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>com.photon.phresco.plugins.dotnet.DotnetPlugin</implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Environment</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
					<class>com.photon.phresco.framework.param.DeployEnvironment</class>
                        <dependencies>
                            <dependency>
								<groupId>com.photon.phresco.commons</groupId>
								<artifactId>phresco-commons</artifactId>
								<type>jar</type>
								<version>1.0.0</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
					<multiple>false</multiple>
					<sort>false</sort>
					<show>true</show>
                    <value></value>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>showErrors</key>
                            <value>-e</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>hideLogs</key>
                            <value>-q</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>showDebug</key>
                            <value>-X</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>logs</key>
                    <possibleValues>
                        <value key="showErrors">Show Errors</value>
                        <value key="hideLogs">Hide Logs</value>
                        <value key="showDebug">Show Debug</value>
                    </possibleValues>
                    <multiple>false</multiple>
					<sort>false</sort>
					<show>true</show>
                    <value></value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>Hidden</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildNumber</key>
					<multiple>false</multiple>
					<sort>false</sort>
					<show>false</show>
                    <value></value>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
	<mojo>
      <goal>validate-code</goal>
      <description>Goal which validates the DotNet project</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
	  <configuration>
      <parameters>
        <parameter>
			<name>
               <value lang="en">Validate Against</value>
            </name>
			<type>List</type>
			<required>false</required>
			<editable>true</editable>
			<description></description>
			<key>sonar</key>
			<possibleValues>
				<value key="src" dependency="skipTests">Source</value>
				<value key="functional">Functional Test</value>
			</possibleValues>
			<multiple>false</multiple>
			<sort>false</sort>
			<show>true</show>
		  <value></value>
        </parameter>
		<parameter>
			<pluginParameter>framework</pluginParameter>
			<mavenCommands>
				<mavenCommand>
					<key>true</key>
					<value>-DskipTests=true</value>
				</mavenCommand>
				<mavenCommand>
					<key>false</key>
					<value>-DskipTests=false</value>
				</mavenCommand>
			</mavenCommands>
			<name>
				<value lang="en">Skip Unit Test</value>
			</name>
			<type>Boolean</type>
			<required>false</required>
			<editable>true</editable>
			<description></description>
			<key>skipTests</key>
			<multiple>false</multiple>
			<sort>false</sort>
			<show>false</show>
			<value>false</value>
        </parameter>
    </parameters>
    </configuration>
    </mojo>
	<mojo>
        <goal>CIPrebuildStep</goal>
        <description>Goal which do's CI pre build step</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>com.photon.phresco.plugins.CIPlugin</implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
    </mojo>
</mojos>
