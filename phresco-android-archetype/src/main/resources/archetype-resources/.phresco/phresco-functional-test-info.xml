<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
        <goal>functional-test-webdriver</goal>
        <description>Goal which functional Test-webdriver the Android Native project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>
           <dependency>
               <groupId>com.photon.phresco.plugins</groupId>
               <artifactId>android-phresco-plugin</artifactId>
               <type>jar</type>
               <version>2.1.0.1001-SNAPSHOT</version>
               <class>com.photon.phresco.plugins.android.AndroidPlugin</class>
           </dependency>
       </implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <pluginParameter>plugin</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>usb</key>
                            <value>-Dandroid.device="usb"</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>emulator</key>
                            <value>-Dandroid.device="emulator"</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Devices</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>devices</key>
                    <possibleValues>
                        <value key="usb">USB</value>
                        <value key="emulator">Emulator</value>
                        <value key="serialNumber" dependency="serialNumber">Serial Number</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <sort>false</sort>
                    <show>true</show>
                    <value></value>
                </parameter>
				<parameter>
                    <name>
                        <value lang="en">Serial Number</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>serialNumber</key>
					<multiple>false</multiple>
					<sort>false</sort>
                    <show>false</show>
                    <value></value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Signing</value>
                    </name>
                    <type>boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>signing</key>
                    <multiple>false</multiple>
                    <sort>false</sort>
                    <show>true</show>
                    <value>false</value>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
</mojos>