<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
        <goal>package</goal>
        <description>Goal which packages the Java-quickstart project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>com.photon.phresco.plugins.java.JavaPlugin</implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Build Name</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>userBuildName</key>
                    <value></value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>Number</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>userbuildNumber</key>
                    <value></value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Environment</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.param.impl.EnvironmentParameter</class>
                        <dependencies>
                            <dependency>
<groupId>com.photon.phresco.commons</groupId>
<artifactId>phresco-commons</artifactId>
<type>jar</type>
<version>1.0.0</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
                    <value>Production</value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Jar Name</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>jarName</key>
                    <value></value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">MainClass Name</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>mainClassName</key>
                    <value>com.photon.phresco.HelloWorld</value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>logs</key>
                    <possibleValues>
                        <value>Show Errors</value>
                        <value>Hide Logs</value>
                        <value>Show Debug</value>
                    </possibleValues>
                    <value>Show Debug</value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Skip UnitTest</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>skipTest</key>
                    <value>true</value>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
    <mojo>
        <goal>deploy</goal>
        <description>Goal which deploys the Java-quickstart project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>com.photon.phresco.plugins.java.JavaPlugin</implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Environment</value>
                        <value lang="cv">Envit</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.param.impl.EnvironmentParameter</class>
                        <dependencies>
                            <dependency>
<groupId>com.photon.phresco.commons</groupId>
<artifactId>phresco-commons</artifactId>
<type>jar</type>
<version>1.0.0</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
                    <value>Production</value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>logs</key>
                    <possibleValues>
                        <value>Show Errors</value>
                        <value>Hide Logs</value>
                        <value>Show Debug</value>
                    </possibleValues>
                    <value>Show Debug</value>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
	<mojo>
      <goal>validate-code</goal>
      <description>Goal which validates the Java-quickstart project</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
	  <configuration>
      <parameters>
        <parameter>
		<pluginParameter>plugin</pluginParameter>
			<mavenCommands>
				<mavenCommand>
					<key>skipTests</key>
					<value>-DskipTests</value>
				</mavenCommand>
			</mavenCommands>
			<name>codeValidation</name>
			<type>java.lang.String</type>
			<required>false</required>
			<editable>true</editable>
			<description></description>
			<key>sonar</key>
			<possibleValues>
				<value dependency="src,skipTests">src</value>
				<value>functional</value>
			</possibleValues>
		  <value></value>
        </parameter>
		<parameter>
			<name>src</name>
			<mavenCommands>
				<mavenCommand>
					<key>js</key>
					<value>-Pjs</value>
				</mavenCommand>
				<mavenCommand>
					<key>java</key>
					<value>-Pjava</value>
				</mavenCommand>
				<mavenCommand>
					<key>html</key>
					<value>-Phtml</value>
				</mavenCommand>
				<mavenCommand>
					<key>jsp/jsf</key>
					<value>-Pweb</value>
				</mavenCommand>
			</mavenCommands>
			<pluginParameter>plugin</pluginParameter>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
		  <key>src</key>
			<possibleValues>
				<value>js</value>
				<value>java</value>
				<value>html</value>
				<value>jsp/jsf</value>
			</possibleValues>
		  <value></value>
        </parameter>
          </parameters>
      </configuration>
    </mojo>
</mojos>
