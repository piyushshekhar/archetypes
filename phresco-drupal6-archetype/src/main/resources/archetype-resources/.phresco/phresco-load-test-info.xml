<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
		<goal>load-test</goal>
		<description>Goal which LoadTest the Drupal-6 project</description>
		<requiresDirectInvocation>false</requiresDirectInvocation>
		<requiresProject>true</requiresProject>
		<requiresReports>false</requiresReports>
		<aggregator>false</aggregator>
		<requiresOnline>false</requiresOnline>
		<inheritedByDefault>true</inheritedByDefault>
		<implementation>com.photon.phresco.plugins.drupal.DrupalPlugin</implementation>
		<language>java</language>
		<instantiationStrategy>per-lookup</instantiationStrategy>
		<executionStrategy>once-per-session</executionStrategy>
		<threadSafe>false</threadSafe>
			<configuration>
			<parameters>
				<parameter>
					<name>
						<value lang="en">Test Against</value>
					</name>
					<type>List</type>
					<required>false</required>
					<editable>true</editable>
					<description></description>
					<key>testAgainst</key>
					<possibleValues>
						<value key="Server" dependency="server">Server</value>
						<value key="WebService" dependency="webservice">WebService</value>
					</possibleValues>
					<value></value>
					<sort>false</sort>
					<show>true</show>
				</parameter>
				<parameter>
					<name>
						<value lang="en">Show Settings</value>
					</name>
					<type>Boolean</type>
					<required>false</required>
					<editable>true</editable>
					<description></description>
					<key>showSettings</key>
					<multiple>false</multiple>
					<value>false</value>
					<sort>false</sort>
					<show>true</show>
					<dependency>environmentName</dependency>
				</parameter>
				<parameter>
					<name>
						<value lang="en">Environment</value>
					</name>
					<type>DynamicParameter</type>
					<dynamicParameter>
						<class>com.photon.phresco.impl.EnvironmentsParameterImpl</class>
						<dependencies>
							<dependency>
								<groupId>com.photon.phresco.commons</groupId>
								<artifactId>phresco-commons</artifactId>
								<type>jar</type>
								<version>1.0.0</version>
							</dependency>
						</dependencies>
					</dynamicParameter>
					<required>true</required>
					<editable>true</editable>
					<key>environmentName</key>
					<multiple>false</multiple>
					<value></value>
					<sort>false</sort>
					<show>true</show>
					<dependency>Server,WebService</dependency>
				</parameter>
				<parameter>
					<name>
						<value lang="en">Server</value>
					</name>
					<type>DynamicParameter</type>
					<dynamicParameter>
						<class>com.photon.phresco.impl.DynamicServerImpl</class>
							<dependencies>
							<dependency>
								<groupId>com.photon.phresco.commons</groupId>
								<artifactId>phresco-commons</artifactId>
								<type>jar</type>
								<version>1.0.0</version>
							</dependency>
						</dependencies>
					</dynamicParameter>
					<required>true</required>
					<editable>true</editable>
					<description></description>
					<key>Server</key>
					<multiple>false</multiple>
					<value></value>
					<sort>false</sort>
					<show>true</show>
				</parameter>
				<parameter>
					<name>
						<value lang="en">WebService</value>
					</name>
					<type>DynamicParameter</type>
					<dynamicParameter>
						<class>com.photon.phresco.impl.DynamicServerImpl</class>
						<dependencies>
							<dependency>
								<groupId>com.photon.phresco.commons</groupId>
								<artifactId>phresco-commons</artifactId>
								<type>jar</type>
								<version>1.0.0</version>
							</dependency>
						</dependencies>
					</dynamicParameter>
					<required>true</required>
					<editable>true</editable>
					<description></description>
					<key>WebService</key>
					<multiple>false</multiple>
					<value></value>
					<sort>false</sort>
					<show>false</show>
				</parameter>
				<parameter>
					<name>
						<value lang="en">Test Result Name</value>
					</name>
					<type>String</type>
					<required>true</required>
					<editable>true</editable>
					<description></description>
					<key>testName</key>
					<multiple>false</multiple>
					<value></value>
					<sort>false</sort>
					<show>true</show>
				</parameter>
				<parameter>
					<name>
						<value lang="en">NoOfUsers</value>
					</name>
					<type>Number</type>
					<required>true</required>
					<editable>true</editable>
					<description></description>
					<key>noOfUsers</key>
					<multiple>false</multiple>
					<value></value>
					<sort>false</sort>
					<show>true</show>
				</parameter>
				<parameter>
					<name>
						<value lang="en">Ramp-Up Period</value>
					</name>
					<type>String</type>
					<required>true</required>
					<editable>true</editable>
					<description></description>
					<key>rampUpPeriod</key>
					<multiple>false</multiple>
					<value></value>
					<sort>false</sort>
					<show>true</show>
				</parameter>
				<parameter>
					<name>
						<value lang="en">Loop Count</value>
					</name>
					<type>String</type>
					<required>true</required>
					<editable>true</editable>
					<description></description>
					<key>loopCount</key>
					<multiple>false</multiple>
					<value></value>
					<sort>false</sort>
					<show>true</show>
				</parameter>
				<parameter>
					<name>
						<value lang="en">Add Header</value>
					</name>
					<type>map</type>
					<required>true</required>
					<editable>true</editable>
					<description></description>
					<key>addHeader</key>
					<multiple>true</multiple>
					<value></value>
					<sort>false</sort>
					<show>true</show>
				</parameter>
			</parameters>
		</configuration>
	</mojo>
</mojos>